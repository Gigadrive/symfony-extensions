{
	"name": "gigadrive/symfony-extensions",
	"version": "0.1.0",
	"description": "A bundle with Symfony 5 utilities by Gigadrive.",
	"minimum-stability": "stable",
	"license": "GPL-3.0-only",
	"keywords": [
		"Symfony",
		"Extension",
		"Basic",
		"Essentials",
		"Gigadrive"
	],
	"type": "symfony-bundle",
	"authors": [
		{
			"name": "Mehdi Baaboura",
			"email": "mbaaboura@gigadrivegroup.com"
		}
	],
	"homepage": "https://gitlab.com/Gigadrive/symfony-extensions",
	"readme": "README.md",
	"require": {
		"php": "^7.2 || ^8.0",
		"symfony/framework-bundle": "~5.0",
		"symfony/orm-pack": "^1.0",
		"doctrine/annotations": "^1.10",
		"symfony/yaml": "^5.1",
		"symfony/security-csrf": "^5.1",
		"composer/installers": "^1.9",
		"neitanod/forceutf8": "^2.0",
		"symfony/twig-bundle": "^5.1",
		"gumlet/php-image-resize": "^2.0",
		"ext-json": "*",
		"gigadrive/php-commons": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Gigadrive\\Bundle\\SymfonyExtensionsBundle\\": "src/"
		},
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Gigadrive\\Bundle\\SymfonyExtensionsBundle\\Tests\\": "Tests"
		}
	},
	"config": {
		"platform": {
			"php": "7.2.5"
		}
	}
}
